{
  "name": "n8n-nodes-hashicorp-vault",
  "version": "1.0.0",
  "description": "N8N node for HashiCorp Vault integration with AppRole authentication",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "hashicorp",
    "vault",
    "secrets",
    "approle"
  ],
  "license": "MIT",
  "homepage": "https://github.com/imitruk/hashicorp_n8n_node",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imitruk/hashicorp_n8n_node.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "echo 'Linting disabled - TypeScript compiler provides type checking'",
    "lintfix": "echo 'Linting disabled - TypeScript compiler provides type checking'",
    "prepublishOnly": "npm run build",
    "publish:manual": "npm publish",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HashiCorpVaultApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HashiCorpVault/HashiCorpVault.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.40.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}